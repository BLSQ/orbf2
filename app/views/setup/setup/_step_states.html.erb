<% if step.model.id%>
<div class="actions pull-right">
  <%= link_to 'New State', new_setup_project_state_path(project), class: 'btn btn-info' %>
</div>
<table class="table table-striped table-condensed">
    <thead>
        <tr>
            <th>Name</th>
            <th>Short Name</th>
            <th>Code</th>
        </tr>
    </thead>
    <body>
        <%= render partial: "state", collection: step.model.states, as: :state, locals: {readonly: readonly} %>
    </body>
</table>
<% end%>
