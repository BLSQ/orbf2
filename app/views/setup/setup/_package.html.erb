<tr>
    <td><%= link_to package.name , edit_setup_project_package_path(project,package)%></td>
    <td><small><%= package.frequency=%></small></td>
    <td>
        <small><%= package.package_entity_groups.map{ |entitygroup|  entitygroup.name }.join(', ') %></small>
    </td>
    <td>
        <small><%= package.states.map{ |state| state.name }.join(', ') %></small>
    </td>
    <td>
      <% package.activity_packages.map{ |ap| ap.activity.name }.sort.each do |activity_name| %>
        <li><small><%= activity_name %></small></li>
      <%end%>
    </td>
</tr>
