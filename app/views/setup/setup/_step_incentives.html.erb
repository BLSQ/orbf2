<%if project &&  project.id%>
 <% if project.missing_activity_states.values.flatten.any? %>
  <%= link_to 'Let\'s create missing dhis2 elements', mass_creation_setup_project_activities_path(project), class: 'btn btn-danger'  %>
 <% elsif step.model%>
  <%= link_to 'New Configuration', new_setup_project_incentive_path(project), class: 'btn btn-info pull-right' %>
  <table class="table table-striped table-condensed">
      <thead>
          <tr>
              <th>headers...</th>
          </tr>
      </thead>
      <body>
          <%= render partial: "incentive", collection: step.model %>
      </body>
  </table>
  <% else %>
    <%= link_to 'Let\'s configure incentives', new_setup_project_incentive_path(project)  %>
  <% end %>
<%end%>
