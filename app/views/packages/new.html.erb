<%= simple_form_for(package,  url: setup_project_packages_path(current_user.project), method: :post) do |f| %>
<%= f.input :name %>
<div class="row">
    <div class="col-md-6">
        <select id="data_elements_selector" data-placeholder="Lookup the data elements here..." data-selection = "data_elements_selection" data-url = "<%= data_elements_setup_project_autocomplete_index_path(current_user.project) %>" class="form-control" name="character" multiple="multiple"></select>
    </div>
    <div class="col-md-6" id="data_elements_selection"></div>
</div>
<%= f.input :data_element_group_ext_ref %>
<%= f.input :frequency %>
<%= f.button :submit, class: "btn btn-success" %>
<% end %>
